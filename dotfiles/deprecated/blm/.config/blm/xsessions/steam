# Add system defaults
if [ -d /etc/X11/xinit/xinitrc.d ]; then
    for f in /etc/X11/xinit/xinitrc.d/*; do
        [ -x "$f" ] && . "$f"
    done
    unset f
fi

# Merge user Xresources
[[ -f ~/.Xresources ]] && xrdb -display $DISPLAY -merge ~/.Xresources

xrandr --newmode 1600x900_60.00  118.25  1600 1696 1856 2112  900 903 908 934\
       -hsync +vsync
xrandr --addmode eDP1 1600x900_60.00
xrandr -s 1600x900_60.00

exec openbox-session
