{
  "name": "Cyan Scrollbar",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Scrollbar for the cyan exwm theme",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [ "\u003Call_urls\u003E" ],
      "js": ["js/scrollbar.js"],
      "css": [ "css/scrollbar.css" ],
      "run_at": "document_start"
    }
  ]
}
